ohio.func(example, ohio.make(GUI) \n ohio.add.code(name=GUI code="#!/usr/bin/env python3") \n ohio.add.code(name=GUI code="import tkinter as tk") \n ohio.add.code(name=GUI code="root = tk.Tk()") \n ohio.add.code(name=GUI code="tk.Label(root, text='lol', pady=10).pack()") \n ohio.add.code(name=GUI code="root.mainloop()") \n ohio.exec(chmod +x GUI) \n ohio.exec(./GUI 2>/dev/null 1>/dev/null))
